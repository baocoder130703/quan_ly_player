        <input type="number" id="shoot" name="luc_sut">
        <br>
        <label for="id_father">father</label>
        <select name="type_id" id="type">
            <% todos.forEach((todo) => { %>
                <option value="<%= todo.id %>"><%= todo.title %></option>
            <% }) ;%>
        </select>
        <br>
        <label for="">image</label>
        <input type="file" name="image">
        <input type="submit" >
    </form>
